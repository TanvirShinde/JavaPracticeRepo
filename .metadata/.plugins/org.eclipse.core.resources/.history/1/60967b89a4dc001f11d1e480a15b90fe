package Vagaro;

public class Compare {
	public static void main(String[] args) {
        Compare m = new Compare();
        m.compare(550,450,750);
    }
	void compare(int num1 ,int num2,int num3){
        if(num1>num2)
        {
            if(num1>num3){
               System.out.println(num1+" is a greatest number"); 
            }else{
                System.out.println(num3+" is a greatest number");
            }
        }else if(num2>num3)
        {
            System.out.println(num2+" is a greatest number");
        }else 
        {
            System.out.println(num3+" is a greatest number");
        }
    }
}

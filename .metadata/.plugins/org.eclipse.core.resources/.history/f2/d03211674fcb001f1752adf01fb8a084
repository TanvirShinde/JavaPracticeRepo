package program_on_patterns;

public class StarPattern8 {
	
	void starPattern8() {
		for(int i=1;i<=3;i++) {
			char ch='A';
			for(int j=1;j<=3;j++) {
				System.out.print(ch);
				ch++;
			}
			System.out.println(); 
		}
	}

	public static void main(String args[ ]){
		StarPattern8 star8 = new StarPattern8();
		star8.starPattern8();
		
	}
}

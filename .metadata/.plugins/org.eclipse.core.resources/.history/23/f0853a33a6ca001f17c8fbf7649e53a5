package patternPrograms;
public class Assignment42 {

	void patternPrinting() {

		for(int i=1;i<=4;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(i +" ");	

			}System.out.println();
		}
	}
	public static void main(String[] args) {
		IncreaseSameNumberByRow pattern5=new IncreaseSameNumberByRow();
		pattern5.patternPrinting();

	}

}